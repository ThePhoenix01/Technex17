        android:orientation="horizontal"
        android:layout_height="wrap_content">

        <android.support.v7.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:custom="http://schemas.android.com/apk/res-auto"
            android:layout_height="160dp"
            custom:cardBackgroundColor="#00000000"

            android:layout_margin="4dp"
            android:layout_weight="1"
            android:layout_width="0dp">

            <FrameLayout
                android:layout_height="match_parent"
                android:id="@+id/cv1"
                android:layout_width="match_parent"
                android:background="@color/md_red_trans">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="50dp">
                    <Space
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="50dp" />
                    <ImageView
                        android:layout_width="50dp"
                        android:padding="5dp"
                        android:layout_height="50dp"
                        android:src="@drawable/robo_white"
                        android:scaleType="fitCenter"
                        />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_gravity="bottom|start"
                    android:orientation="vertical"
                    android:layout_margin="10dp"
                    android:layout_height="wrap_content">

                    <org.piratejester.technex17.utils.BoldModTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|end"
                        android:textSize="16dp"
                        android:textColor="@color/white"
                        android:text="Events"/>

                    <org.piratejester.technex17.utils.ModTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="14dp"
                        android:layout_gravity="bottom|end"
                        android:textColor="@color/white"
                        android:text="Ready. Steady. Aim for Glory"/>


                </LinearLayout>
            </FrameLayout>

        </android.support.v7.widget.CardView>
